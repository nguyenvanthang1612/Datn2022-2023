<?xml version="1.0"?>
<routes xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Webapi:etc/webapi.xsd">
    <route url="/V1/ghn/estimate" method="POST">
        <service class="Magenest\GiaoHangNhanh\Api\GiaoHangNhanhInterface" method="estimateShipping"/>
        <resources>
            <resource ref="anonymous" />
        </resources>
        <data>
            <parameter name="cardId">%cart_id%</parameter>
            <parameter name="street">%street%</parameter>
            <parameter name="region">%region%</parameter>
            <parameter name="city">%city%</parameter>
        </data>
    </route>
</routes>