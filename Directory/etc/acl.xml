<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Acl/etc/acl.xsd">
    <acl>
        <resources>
            <resource id="Magento_Backend::admin">
                <resource id="Magenest_Backend::Magenest" title="Magenest" translate="title">
                    <resource id="Magenest_Directory::menu" title="Directory" translate="title" sortOrder="90">
                        <resource id="Magenest_Directory::city" title="City" translate="title" sortOrder="10"/>
                        <resource id="Magenest_Directory::district" title="District" translate="title" sortOrder="20"/>
                        <resource id="Magenest_Directory::ward" title="Ward" translate="title" sortOrder="30"/>
                    </resource>
                </resource>
                <resource id="Magento_Backend::stores">
                    <resource id="Magento_Backend::stores_settings">
                        <resource id="Magento_Config::config">
                            <resource id="Magenest_Directory::configuration" title="Directory Configuration" translate="title" sortOrder="100"/>
                        </resource>
                    </resource>
                </resource>
            </resource>
        </resources>
    </acl>
</config>
